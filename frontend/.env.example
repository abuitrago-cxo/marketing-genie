# AI Agent Assistant Frontend - Environment Configuration

# Auth0 Configuration (for Vite/Playwright, these VITE_ prefixed variables take precedence)
VITE_AUTH0_DOMAIN=your-vite-auth0-domain.auth0.com
VITE_AUTH0_CLIENT_ID=your_vite_auth0_client_id
VITE_AUTH0_AUDIENCE=https://your-vite-auth0-domain.auth0.com/api/v2/

# Auth0 Configuration (fallback for CRA or other environments)
REACT_APP_AUTH0_DOMAIN=your-auth0-domain.auth0.com
REACT_APP_AUTH0_CLIENT_ID=your_auth0_client_id
REACT_APP_AUTH0_AUDIENCE=https://your-auth0-domain.auth0.com/api/v2/

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8000
REACT_APP_WS_BASE_URL=ws://localhost:8000

# Application Configuration
REACT_APP_APP_NAME=AI Agent Assistant
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Feature Flags
REACT_APP_ENABLE_GITHUB_INTEGRATION=true
REACT_APP_ENABLE_SPECIALIZED_AGENTS=true
REACT_APP_ENABLE_ANALYTICS=false

# Debug Configuration
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info
