# Google Gemini API Key
# GEMINI_API_KEY=

# OpenAI API Key (for OpenAI provider)
# OPENAI_API_KEY=

# OpenAI-Compatible API Configuration
# LLM_PROVIDER=openai_compatible  # Options: google, openai, openai_compatible
# OPENAI_COMPATIBLE_BASE_URL=http://localhost:1234/v1
# OPENAI_COMPATIBLE_API_KEY=your-api-key

# Search Tool Configuration
# SEARCH_TOOL=firecrawl  # Options: google, firecrawl, none
# FIRECRAWL_API_KEY=your-firecrawl-api-key
# FIRECRAWL_BASE_URL=https://api.firecrawl.dev
# MAX_SEARCH_RESULTS=5
# MAX_CONTENT_LENGTH=4000

# Model Configuration (can be overridden for any provider)
# QUERY_GENERATOR_MODEL=gpt-3.5-turbo  # For OpenAI/compatible
# REFLECTION_MODEL=gpt-3.5-turbo       # For OpenAI/compatible  
# ANSWER_MODEL=gpt-4                   # For OpenAI/compatible